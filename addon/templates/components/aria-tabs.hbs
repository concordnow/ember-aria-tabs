{{! template-lint-disable no-action }}
{{yield
    (hash
      tabList=(component "aria-tab-list"
        disabledTabClassName=this.disabledTabClassName
        focus=this.focus
        onClick=(action "handleClick")
        onKeyDown=(action "handleKeyDown")
        panelNodes=this.panelNodes
        parentGuid=this.elementId
        selectedIndex=this.selectedIndex
        selectedTabClassName=this.selectedTabClassName
        tabNodes=this.tabNodes
      )
      tabPanel=(component "aria-tab-panel"
        forceRender=this.forceRenderTabPanel
        panelNodes=this.panelNodes
        parentGuid=this.elementId
        selectedTabPanelClassName=this.selectedTabPanelClassName
        selectedIndex=this.selectedIndex
        tabNodes=this.tabNodes
      )
    )
}}